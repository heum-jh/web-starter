{
  "name": "50k-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "generate": "graphql-codegen --config",
    "prepare": "husky install",
    "build:check": "tsc --noEmit",
    "lint:check": "eslint -f stylish \"src/**/*.{ts,tsx}\"",
    "format:check": "prettier -c \"src/**/*.{ts,tsx}\"",
    "fix:prettier": "prettier -w \"src/**/*.{ts,tsx}\""
  },
  "dependencies": {
    "@apollo/client": "^3.8.4",
    "@types/luxon": "^3.3.2",
    "@types/node": "20.7.0",
    "@types/react": "18.2.22",
    "@types/react-dom": "18.2.7",
    "autoprefixer": "10.4.16",
    "clsx": "^2.0.0",
    "eslint": "8.50.0",
    "eslint-config-next": "13.5.3",
    "graphql": "^16.8.1",
    "luxon": "^3.4.3",
    "next": "13.5.3",
    "postcss": "8.4.30",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwind-merge": "^1.14.0",
    "tailwindcss": "3.3.3",
    "typescript": "^5.2.2"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/client-preset": "^4.1.0",
    "@graphql-codegen/typescript-react-apollo": "^4.0.0",
    "@types/navermaps": "^3.7.0",
    "dotenv": "^16.3.1",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "prettier": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.4",
    "ts-node": "^10.9.1"
  },
  "lint-staged": {
    "src/*.{ts,tsx}": [
      "prettier --write",
      "eslint --max-warnings=0"
    ]
  }
}
